{
  "name": "@cdktf/cli-core",
  "version": "0.0.0",
  "description": "CDK for Terraform CLI Core, meant for internal use only",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "watch-preserve-output": "tsc -w --preserveWatchOutput",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "pretest": "yarn build",
    "pretest:ci": "yarn build",
    "test": "yarn lint && jest",
    "test:ci": "yarn lint && jest --ci",
    "jest-watch": "jest --watch",
    "package": "./package.sh",
    "dist-clean": "rm -rf dist"
  },
  "main": "src/lib/index.js",
  "types": "src/lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hashicorp/terraform-cdk.git",
    "directory": "packages/@cdktf/cli-core"
  },
  "author": {
    "name": "HashiCorp",
    "url": "https://hashicorp.com"
  },
  "keywords": [
    "cdk",
    "cdktf",
    "terraform"
  ],
  "license": "MPL-2.0",
  "dependencies": {
    "@cdktf/commons": "0.0.0",
    "@cdktf/hcl2cdk": "0.0.0",
    "@cdktf/provider-schema": "0.0.0",
    "@cdktf/node-pty-prebuilt-multiarch": "0.10.1-pre.11",
    "@sentry/node": "^7.80.0",
    "cdktf": "0.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "ci-info": "^3.9.0",
    "codemaker": "^1.91.0",
    "constructs": "^10.0.25",
    "fs-extra": "^8.1.0",
    "https-proxy-agent": "^5.0.1",
    "indent-string": "^4.0.0",
    "jsii": "^5.2.29",
    "jsii-pacmak": "^1.91.0",
    "jsii-srcmak": "^0.1.964",
    "minimatch": "^5.1.6",
    "node-fetch": "^2.7.0",
    "pkg-up": "^3.1.0",
    "semver": "^7.5.4",
    "sscaff": "^1.2.274",
    "strip-ansi": "^6.0.1",
    "xml-js": "^1.6.11",
    "xstate": "^4.38.3",
    "yoga-layout-prebuilt": "^1.10.0",
    "zod": "^3.22.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-use-before-define": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-var-requires": 0,
      "no-sequences": "error",
      "no-irregular-whitespace": [
        "error",
        {
          "skipTemplates": true
        }
      ]
    },
    "ignorePatterns": [
      "node_modules",
      "dist",
      "coverage"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "tsc-files ambient.d.ts --noEmit"
  },
  "devDependencies": {
    "@cdktf/provider-generator": "0.0.0",
    "@types/archiver": "^5.3.4",
    "@types/cross-spawn": "^6.0.5",
    "@types/detect-port": "^1.3.5",
    "@types/follow-redirects": "^1.14.4",
    "@types/fs-extra": "^8.1.5",
    "@types/ink": "^2.0.3",
    "@types/ink-spinner": "^3.0.4",
    "@types/jest": "^29.5.8",
    "@types/json-schema": "^7.0.15",
    "@types/lodash.isequal": "^4.5.8",
    "@types/nock": "^11.1.0",
    "@types/node": "18.18.9",
    "@types/node-fetch": "2.6.9",
    "@types/parse-gitignore": "^1.0.2",
    "@types/semver": "^7.5.5",
    "@types/stream-buffers": "^3.0.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "esbuild": "^0.19.5",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "jest": "^29.7.0",
    "lint-staged": ">=10",
    "nock": "^13.3.8",
    "ts-jest": "^29.1.1",
    "tsc-files": "^1.1.4",
    "typescript": "^5.0.4"
  }
}
